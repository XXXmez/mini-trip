{
  "name": "mini-trip",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "init": "npm install",
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "vite build",
    "build:ci": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "type-check": "tsc --noEmit",
    "check": "npm run format && npm run lint && npm run type-check",
    "clean": "rimraf node_modules dist .turbo .vite",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "fastest-levenshtein": "^1.0.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "vite-plugin-svgr": "^4.3.0",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@feature-sliced/eslint-config": "^0.1.1",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.3.3",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "prettier": "^3.3.3",
    "prettier-plugin-css-order": "^2.1.2",
    "sass": "^1.86.3",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^7.1.5",
    "vite-plugin-pwa": "^1.0.3"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --ext .ts,.tsx,.js,.jsx --fix"
    ],
    "src/**/*.{css,scss,md,json}": [
      "prettier --write"
    ]
  }
}
